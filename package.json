{
  "name": "studymateproject",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "prisma generate",
    "lint": "eslint",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "precompute:matches": "tsx scripts/precompute-matches.ts",
    "precompute:dev": "NODE_ENV=development tsx scripts/precompute-matches.ts",
    "redis:check": "tsx -e \"import { SimpleRedis } from './lib/cache/redis-simple'; SimpleRedis.isHealthy().then(h => console.log('Redis healthy:', h))\"",
    "redis:check:full": "tsx -e \"import { RedisCache } from './lib/cache/redis-client'; RedisCache.getInstance().isHealthy().then(h => console.log('Redis healthy:', h))\"",
    "matching:debug": "tsx -e \"import { SmartMatchingEngine } from './lib/matching/smart-engine'; console.log('Performance metrics:', SmartMatchingEngine.getPerformanceMetrics())\"",
    "test:pusher": "npx tsx scripts/test-pusher-messaging.ts && npx tsx scripts/test-pusher-errors.ts && npx tsx scripts/test-pusher-performance.ts",
    "test:pusher:core": "npx tsx scripts/test-pusher-messaging.ts",
    "test:pusher:errors": "npx tsx scripts/test-pusher-errors.ts",
    "test:pusher:perf": "npx tsx scripts/test-pusher-performance.ts",
    "test:monitoring": "npx tsx scripts/test-monitoring.ts",
    "diagrams:setup": "./scripts/setup-plantuml.sh",
    "diagrams:export": "./scripts/export-diagrams.sh",
    "diagrams:png": "plantuml -tpng -o exported docs/*.puml",
    "diagrams:svg": "plantuml -tsvg -o exported docs/*.puml"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.1.5",
    "@hookform/resolvers": "^3.10.0",
    "@prisma/client": "^6.16.2",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.4",
    "@types/pako": "^2.0.4",
    "@types/react-window": "^1.8.8",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.15.0",
    "idb": "^8.0.3",
    "ioredis": "^5.8.0",
    "lucide-react": "^0.468.0",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "pako": "^2.1.0",
    "pusher": "^5.2.0",
    "pusher-js": "^8.4.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.54.3",
    "react-hot-toast": "^2.4.1",
    "react-window": "^2.2.2",
    "redis": "^5.9.0",
    "simple-peer": "^9.11.1",
    "swr": "^2.3.6",
    "zod": "^3.24.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.19.17",
    "@types/nodemailer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/simple-peer": "^9.11.8",
    "@types/uuid": "^10.0.0",
    "@vitest/ui": "^4.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "fake-indexeddb": "^6.2.4",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "prisma": "^6.16.2",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "vitest": "^4.0.3"
  }
}
